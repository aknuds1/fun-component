{
  "name": "fun-component",
  "version": "2.0.2",
  "description": "Performant component as function",
  "main": "index.js",
  "scripts": {
    "posttest": "eslint ./ --ignore-pattern node_modules",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "browserify test/browser.js | tape-run | tap-format-spec",
    "test:node": "NODE_ENV=test node test/node.js | tap-format-spec",
    "example:mapbox": "FUN_COMPONENT=../.. npm start --prefix ./examples/mapbox",
    "example:list": "FUN_COMPONENT=../.. npm start --prefix ./examples/list",
    "deploy:mapbox": "npm run deploy --prefix ./examples/mapbox",
    "deploy:list": "npm run deploy --prefix ./examples/list"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tornqvist/fun-component.git"
  },
  "keywords": [
    "component",
    "html",
    "function"
  ],
  "author": "Carl TÃ¶rnqvist <calle.tornqvist@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tornqvist/fun-component/issues"
  },
  "homepage": "https://github.com/tornqvist/fun-component#readme",
  "devDependencies": {
    "@tap-format/spec": "^0.2.0",
    "bel": "^5.1.0",
    "browserify": "^14.4.0",
    "budo": "^10.0.4",
    "envify": "^4.1.0",
    "eslint": "^4.6.1",
    "now": "^8.2.1",
    "tape": "^4.8.0",
    "tape-run": "^3.0.0"
  },
  "dependencies": {
    "nanocomponent": "^6.4.2",
    "nanologger": "^1.3.0"
  }
}
